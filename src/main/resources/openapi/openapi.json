  "paths": {
    "/studeal/boards/{boardId}/teachers/{teacherId}": {
      "put": {
        "tags": [
          "게시판 API"
        ],
        "summary": "게시글 수정",
        "description": "과외 모집 게시글을 수정합니다. 본인이 작성한 게시글만 수정 가능합니다.",
        "operationId": "updateBoard",
        "parameters": [
          {
            "name": "boardId",
            "in": "path",
            "description": "게시글 ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          },
          {
            "name": "teacherId",
            "in": "path",
            "description": "선생님 ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BoardUpdateRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "COMMON200": {
            "description": "OK, 성공",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponseBoardDetailResponse"
                }
              }
            }
          },
          "BOARD4003": {
            "description": "게시글 수정 권한이 없습니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          },
          "BOARD4001": {
            "description": "게시글을 찾을 수 없습니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "게시판 API"
        ],
        "summary": "게시글 삭제",
        "description": "선생님이 작성한 과외 모집 게시글을 삭제합니다.",
        "operationId": "deleteBoard",
        "parameters": [
          {
            "name": "boardId",
            "in": "path",
            "description": "게시글 ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          },
          {
            "name": "teacherId",
            "in": "path",
            "description": "선생님 ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "COMMON200": {
            "description": "OK, 성공",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponseJava.lang.Object"
                }
              }
            }
          },
          "BOARD4002": {
            "description": "게시글 작성자만 삭제할 수 있습니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          },
          "BOARD4001": {
            "description": "게시글을 찾을 수 없습니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          }
        }
      }
    },
    "/studeal/negotiations": {
      "post": {
        "tags": [
          "가격 제안(협상)"
        ],
        "summary": "학생 가격 제안(협상) 생성 API",
        "description": "학생이 강사에게 가격 제안을 생성하는 API입니다.",
