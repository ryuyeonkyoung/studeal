  "components": {
    "schemas": {
      "BoardUpdateRequest": {
        "required": [
          "expectedPrice",
          "major",
          "subject",
          "title"
        ],
        "type": "object",
        "properties": {
          "title": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string",
            "description": "게시글 제목",
            "example": "고등 수학 과외 모집합니다 (수정)"
          },
          "content": {
            "maxLength": 2000,
            "minLength": 0,
            "type": "string",
            "description": "게시글 내용",
            "example": "수학 전공 대학생이 수학 과외를 모집합니다. 문제 풀이 및 개념 설명에 능숙합니다. (내용 수정)"
          },
          "major": {
            "type": "string",
            "description": "게시글 유형",
            "example": "MATH",
            "enum": [
              "MATH",
              "SCIENCE",
              "ENGLISH",
              "KOREAN",
              "HISTORY",
              "CODING"
            ]
          },
          "expectedPrice": {
            "type": "integer",
            "description": "예상 수업 가격",
            "format": "int64",
            "example": 55000
          },
          "subject": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "description": "수업 주제",
            "example": "고등 수학 - 미적분과 통계"
          }
        },
        "description": "게시글 수정 요청"
      },
      "com.studeal.team.global.error.ApiResponse": {
        "type": "object",
        "properties": {
          "isSuccess": {
            "type": "boolean"
          },
          "code": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "result": {
            "type": "object"
          }
        }
      },
      "BoardDetailResponse": {
        "type": "object",
        "properties": {
          "boardId": {
            "type": "integer",
            "description": "게시글 ID",
            "format": "int64",
            "example": 1
          },
          "title": {
            "type": "string",
            "description": "게시글 제목",
            "example": "고등 수학 과외 모집합니다"
          },
          "content": {
            "type": "string",
            "description": "게시글 내용",
            "example": "수학 전공 대학생이 수학 과외를 모집합니다. 문제 풀이 및 개념 설명에 능숙합니다."
          },
          "teacherId": {
            "type": "integer",
            "description": "선생님 ID",
            "format": "int64",
            "example": 1
          },
          "teacherName": {
            "type": "string",
            "description": "선생님 이름",
            "example": "김선생"
          },
          "major": {
            "type": "string",
            "description": "게시글 유형",
            "example": "MATH",
            "enum": [
              "MATH",
              "SCIENCE",
              "ENGLISH",
              "KOREAN",
              "HISTORY",
              "CODING"
            ]
          },
          "expectedPrice": {
            "type": "integer",
            "description": "예상 수업 가격",
            "format": "int64",
            "example": 50000
          },
          "specMajor": {
            "type": "string",
            "description": "구체적인 과외 주제",
            "example": "고등 수학 - 미적분"
          },
          "createdAt": {
            "type": "string",
            "description": "생성 일시",
            "format": "date-time"
