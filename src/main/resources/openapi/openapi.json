              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          },
          "200": {
            "description": "OK, 성공",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponseJava.lang.Object"
